    <title>Cadastro</title>
</head>
<body>
    <header>
        <div id=logotipo><center>
          <a id="logo" href="#home"><img src="/img/logo.jpg" alt="Logo Image"></a></center>
        </div>
        <nav class="main-nav">

          <a href="#cadastro">Cadastrar</a>
          <a href="#alterar">Alterar</a>
        </nav>


      </header>

      <form action="/#cadastro" method="get">
        <div>
            <label for="titulo">Titulo:</label>
            <input type="titulo" name="titulo" id="titulo"placeholder="Digite o titulo" required>
        </div>
        <div>
            <label for="ator">Ator principal:</label>
            <input type="ator" name="ator" id="ator"placeholder="Digite o Ator principal" required>
        </div>
        <div>
            <label for="genero">Genero:</label>
            <input type="genero" name="genero" id="genero" placeholder="Genero:" required>
        </div>
      </form>

</body>
